{
  "name": "boilerplate-express-ts",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "init:migrate": "tsc generate-model.ts && mv generate-model.js generate-model.cjs",
    "make:migrate": "node generate-model.cjs",
    "sequelize": "tsc sequelize.ts && mv sequelize.js sequelize.mjs",
    "sequelize:migrate": "node sequelize.mjs",
    "migrate": "npm run sequelize db:migrate",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "eslint": "eslint ./ --fix",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "db:migrate": "node --loader ts-node/esm ./node_modules/.bin/sequelize-cli db:migrate"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "mysql2": "^3.11.0",
    "oracledb": "^5.5.0",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.13",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.8.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "author": "huda16",
  "license": "ISC",
  "description": ""
}
